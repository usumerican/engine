{
  "name": "engine",
  "version": "0.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "zig:build": "zig build --verbose --summary all --release=fast",
    "zig:run": "zig build run --release=fast",
    "zig:exe": "zig build -Dtarget=x86_64-windows -Dcpu=x86_64_v3 -Dstrip=true --verbose --summary all --release=fast",
    "zig:wasm": "zig build wasm --verbose --summary all --release=small",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lib": "vite build --config vite-lib.config.js"
  },
  "devDependencies": {
    "eslint": "^9.4.0",
    "prettier": "^3.3.0",
    "vite": "^5.2.12"
  }
}
